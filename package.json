{
	"name": "dashboar",
	"version": "1.0.0",
	"license": "MIT",
	"bin": "dist/cli.js",
	"engines": {
		"node": ">=10"
	},
	"scripts": {
		"build": "tsc",
		"start": "npm run build && node dist/cli.js",
		"pretest": "npm run build",
		"test": "ava --verbose"
	},
	"files": [
		"dist/cli.js"
	],
	"dependencies": {
		"bitbucket": "^2.6.3",
		"debug": "^4.3.4",
		"dotenv": "^16.0.0",
		"ink": "^3.0.9",
		"ink-link": "^2.0.0",
		"lodash": "^4.17.21",
		"meow": "^9.0.0",
		"node-fetch": "^2.6.7",
		"pg": "^8.7.3",
		"react": "^16.14.0",
		"tunnel-ssh": "^4.1.6"
	},
	"ava": {
		"typescript": {
			"extensions": [
				"tsx",
				"ts"
			],
			"rewritePaths": {
				"source/": "dist/"
			},
			"compile": false
		}
	},
	"xo": {
		"extends": "xo-react",
		"rules": {
			"react/prop-types": "off"
		}
	},
	"devDependencies": {
		"@ava/typescript": "^2.0.0",
		"@sindresorhus/tsconfig": "^2.0.0",
		"@types/debug": "^4.1.7",
		"@types/lodash": "^4.14.182",
		"@types/node-fetch": "^2.6.1",
		"@types/pg": "^8.6.5",
		"@types/react": "^17.0.26",
		"@types/tunnel-ssh": "^4.1.1",
		"ava": "^3.15.0",
		"chalk": "^4.1.2",
		"eslint-config-xo-react": "^0.25.0",
		"eslint-plugin-react": "^7.26.1",
		"eslint-plugin-react-hooks": "^4.2.0",
		"ink-testing-library": "^2.1.0",
		"sinon": "^13.0.1",
		"typescript": "^4.4.3",
		"xo": "^0.39.1"
	}
}
